{
    "version": 2,
    "builds": [
      {
        "src": "package-lock.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": "nextjs"
}
